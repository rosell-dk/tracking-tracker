{
    "author": "Bj√∏rn Rosell",
    "manifest_version": 3,
    "name": "Tracking tracker",
    "description": "Visualizing tracking activity (shock effect therapy)",
    "version": "1.0",
    "action": {
      "default_icon": "icon.png"
    },
    "permissions": [
      "webRequest",
      "scripting"
    ],
    "host_permissions": [
      "*://*/*"
    ],
    "background": {
      "service_worker": "service-worker.js",
      "type": "module"
    },
    "content_scripts": [
      {
        "js": ["content.js"],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["images/*"],
        "extension_ids": ["*"],
        "matches": [ "*://*/*" ]
      }
    ]
}